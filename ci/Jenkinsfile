pipeline{
    agent any
    environment{
        String secret = 'credetinals'
    }
    stages{
        stage('Git pull'){
            steps{
               echo `${env.JENKINS_URL}`
               echo secret
               
            }
        }
        stage('Npm install'){
            steps{
                echo 'Npm install preocess'
            }
        }
        stage("Build started"){
            steps{
                echo "Build completed...."
                echo "Another change here"
            }
        }
    }
}