pipeline{
    agent any
    stages{
        stage('Git pull'){
            steps{
                def secret= withCredentials('my-secret')
                echo secret
            }
        }
        stage('Npm install'){
            steps{
                echo 'Npm install preocess'
            }
        }
        stage("Build started"){
            steps{
                echo "Build completed...."
                echo "Another change here"
            }
        }
    }
}